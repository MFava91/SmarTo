{
  "extends": "eslint:recommended",
  "env": {
      "es6": true,
      "node": true
  },
  "rules": {
    "arrow-body-style": "off",
    "no-console": "off",
    "no-continue": "off",
    "brace-style": "error",
    "curly": "error",
    "default-case": "error",
    "eqeqeq": "error",
    "guard-for-in": "error",
    "indent": [
        "error",
        2, {
        "VariableDeclarator": 2,
        "SwitchCase": 1
        }
    ],
    "linebreak-style": ["error", "unix"],
    "no-var": "error",
    "no-eval": "error",
    "no-undef": "error",
    "no-undef-init": "error",
    "no-undefined": "error",
    "no-unused-expressions": "error",
    "no-useless-call": "error",
    "no-useless-concat": "error",
    "no-useless-escape": "error",
    "no-void": "error",
    "no-with": "error",
    "one-var": ["error", "never"],
    "quotes": ["error", "single"],
    "radix": "error",
    "semi": ["error", "always"]
  }
}